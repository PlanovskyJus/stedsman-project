<p id="notice"><%= notice %></p>

<div>
  <%= image_tag(@post.avatar.url)%>
</div>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @post.body %>
</p>

<% @post.tags.each do |p| %>
  <%= p.name %>
<% end %>

<p>
<%= render partial: '/comments/form' %>
<%= render partial: '/comments/comment'%>
</p>

<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>
